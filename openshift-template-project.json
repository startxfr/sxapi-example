{
    "kind": "Template",
    "apiVersion": "v1",
    "metadata": {
        "name": "sxapi-example-template-project",
        "annotations": {
            "openshift.io/display-name": "SXAPI example (Workspace)",
            "description": "Create workspace for sxapi-example sandbox",
            "iconClass": "icon-nodejs",
            "tags": "example,startx,alpine,nodejs,project",
            "openshift.io/long-description": "This template create a new project for running sxapi-example files. For more information about using this image, including OpenShift considerations, see https://github.com/startxfr/sxapi-core/blob/master/README.md.",
            "openshift.io/provider-display-name": "STARTX",
            "openshift.io/documentation-url": "https://github.com/startxfr/sxapi-core/blob/master/README.md",
            "openshift.io/support-url": "https://github.com/startxfr/sxapi-core/issues/new"
        }
    },
    "message": "Your project ${APP_NAME} have been created. You can use openshift console to start deploying application into this project.",
    "labels": {
        "template": "sxapi-example-template-project"
    },
    "objects": [
        {
            "kind": "Project",
            "apiVersion": "v1",
            "metadata": {
                "name": "${APP_NAME}",
                "labels": {
                    "app": "${APP_NAME}",
                    "type": "${APP_TYPE}",
                    "provider": "startx",
                    "template": "sxapi-example-template-project"
                },
                "annotations": {
                    "openshift.io/description": "${APP_DESCRIPTION}",
                    "openshift.io/generated-by": "STARTX"
                }
            },
            "spec": {},
            "status": {}
        }
    ],
    "parameters": [
        {
            "name": "APP_NAME",
            "displayName": "Application name",
            "description": "The name of the created workspace",
            "value": "sxapi-example",
            "required": true
        },
        {
            "name": "APP_TYPE",
            "displayName": "Application type",
            "description": "Type of application created into this workspace",
            "value": "application"
        },
        {
            "name": "APP_DESCRIPTION",
            "description": "Description of the workspace",
            "value": "sxapi example application"
        }
    ]
}